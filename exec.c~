#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <string.h>

int main(){//int argc, char * argv[]){
  printf("what would you like to do? ");
  char command[100];
  fgets(command, sizeof(command), stdin);
  int i = 0;
  char* word[100];
  while(command){
    word[i] = strsep(&command, " ");
    i++;
  }
  i = 0;
  while(i < 100){
    printf("%s", word[i]);
    i++;
  }
}
 
